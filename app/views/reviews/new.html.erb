<div class="container">
  <%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.submit "Review", class: "btn btn-primary" %>
  <%= link_to restaurants_path do %>
  <i class="fas fa-home" style="font-size: 40px" ></i>
  <% end %>
  <% end %>
</div>

